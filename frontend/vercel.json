{
  "version": 2,
  "routes": [
    {
      "src": "/serpapi/(.*)",
      "dest": "https://serpapi.com/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-api.com/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_AVIATION_API_KEY": "@aviation_api_key",
    "VITE_SERP_API_KEY": "@serp_api_key",
    "VITE_API_BASE_URL": "@api_base_url"
  }
}
